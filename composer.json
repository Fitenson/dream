{
    "name": "fitenson/dream",
    "description": "UI library for Laravel Blade views inspired by ShadCN UI",
    "type": "library",
    "require": {
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/view": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/filesystem": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/routing": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/container": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/console": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "composer/installers": "^1.12",
        "symfony/string": "^6.0 || ^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fitenson\\Dream\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fitenson\\Dream\\DreamServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fitenson"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
